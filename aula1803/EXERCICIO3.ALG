Algoritmo "Exercicio03"

Var
   numero, quantidadeImpares, contador: inteiro

Inicio
   escreval("Bom dia!!!")
   escreval("Informe 5 números:")
   para contador de 1 ate 5 faca
      leia(numero)
      se (numero%2 = 1) entao
         quantidadeImpares <- quantidadeImpares + 1
      fimse
   fimpara
   escreval("Você incluiu", quantidadeImpares, " números ímpares.")

Fimalgoritmo